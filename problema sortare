        int[] numere = new int[] { 7, 5, 3, 1, 2, 4, 6, 8, 9 };
int itemp = 0;
            for (int i = 0; i<numere.Length-1; i++)
            {


                if (numere[i] > numere[i + 1]) itemp = numere[i + 1]; numere[i + 1] = numere[i]; numere[i] = itemp;







            }

        for (int i = 0; i<numere.Length; i++)
        {

            Console.Write(numere[i]+" ");

        }
